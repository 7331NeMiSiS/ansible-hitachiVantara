[loggers]
keys=root,hv_logger

[handlers]
keys=logfile

[formatters]
keys=logfileformatter

[logger_root]
level=INFO
handlers=logfile

[logger_hv_logger]
level=INFO
handlers=logfile
qualname=hv_logger
propagate=0

[formatter_logfileformatter]
format=%(asctime)s %(name)-9s: %(levelname)s %(message)s

[handler_logfile]
class=handlers.RotatingFileHandler
args=('/var/log/hpe/ansible/hpe_storage_modules.log','a',1000000,20)
formatter=logfileformatter